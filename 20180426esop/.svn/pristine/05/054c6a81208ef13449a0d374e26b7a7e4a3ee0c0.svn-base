/*
 *base style
*/
body, form, frame, iframe, blockquote, p, pre, sup, sub, table, caption, ol, dl, ul, li, dl, dd, dt, div, span, a, input, textarea, fieldset, legend, b, strong, em, i, h1, h2, h3, h4, h5, h6, img, label {
    margin: 0;
    padding: 0;
}
iframe{ border:none;}
table {
    border-collapse: collapse;
    border-spacing: 0;
}
caption, th {
    text-align: center;
}

fieldset, img, abbr, acronym {
    border: 0;
}

a {
    text-decoration: none;
    outline: none;
    cursor: pointer;
	color:#393939;
}

a:hover, a:active {
	color:#0088cc;
    outline: 0;
    text-decoration: none;
}

input {
    outline: none;
}

option {
    padding-left: 0.4em;
}

/* LTR */
select {
    padding: 1px;
}

div:target {
    outline: 0 none;
}

article, aside, details, figcaption, figure,
footer, header, hgroup, nav, section {
    display: block;
}

audio, canvas, video {
    display: inline-block;
}

audio:not([controls]) {
    display: none;
}

fieldset, img {
    border: 0 solid;
}

blockquote:before, blockquote:after, q:before, q:after {
    content: '';
    content: none
}

sub,
sup {
    position: relative;
    font-size: 75%;
    line-height: 0;
    vertical-align: baseline;
}

sup {
    top: -0.5em;
}

sub {
    bottom: -0.25em;
}

button,
input,
select,
textarea {
    margin: 0;
    font-size: 100%;
    vertical-align: middle;
}
button,
input {
    *overflow: visible;
    line-height: normal;
}
textarea {
    overflow: auto;
    vertical-align: top;
}

/*
 *default style
*/
body {
    margin: 0;
    font-family: "Microsoft YaHei",Helvetica,Verdana,Arial,Tahoma;
    font-size: 12px;
    line-height: 20px;
    color: #333333;
	background:none;
	overflow-x:hidden
}
a {
    color: #0088cc;
    text-decoration: none;
}

a:hover {
    color: #25a9fc;
    text-decoration: none;
}
small {
    font-size: 85%;
}

strong {
    font-weight: bold;
}

em {
    font-style: italic;
}

cite {
    font-style: normal;
}
h1,
h2,
h3,
h4,
h5,
h6 {
    /*margin: 10px 0;*/
    font-family: "Microsoft YaHei",Helvetica,Verdana,Arial,Tahoma;
    font-weight: bold;
    line-height: 1;
    color: inherit;
    text-rendering: optimizelegibility;
}
h1 {
    font-size: 36px;
    line-height: 40px;
}

h2 {
    font-size: 30px;
    line-height: 40px;
}

h3 {
    font-size: 24px;
    line-height: 40px;
}

h4 {
    font-size: 18px;
    line-height: 20px;
}

h5 {
    font-size: 14px;
    line-height: 20px;
}

h6 {
    font-size: 12px;
    line-height: 20px;
}
ul,
ol {
    padding: 0;
    margin: 0;
	list-style:none;
}

ul ul,
ul ol,
ol ol,
ol ul {
    margin-bottom: 0;
}

li {
    /*line-height: 20px;*/
}
dl {
    /*margin-bottom: 20px;*/
}

dt,
dd {
    line-height: 20px;
}

dt {
    font-weight: bold;
}

dd {
    /*margin-left: 10px;*/
}
hr {
    margin: 20px 0;
    border: 0;
    border-top: 1px solid #eeeeee;
    border-bottom: 1px solid #ffffff;
}
blockquote {
    padding: 0 0 0 15px;
    margin: 0 0 20px;
    border-left: 5px solid #eeeeee;
}

blockquote small {
    display: block;
    line-height: 20px;
    color: #999999;
}
code,
pre {
    padding: 0 3px 2px;
    font-family: Monaco, Menlo, Consolas, "Courier New", monospace;
    font-size: 12px;
    color: #333333;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
}
code {
    padding: 2px 4px;
    color: #d14;
    background-color: #f7f7f9;
    border: 1px solid #e1e1e8;
}
pre {
    display: block;
    padding: 9.5px;
    margin: 5px 0;
    font-size: 13px;
    line-height: 20px;
    word-break: break-all;
    word-wrap: break-word;
    white-space: pre;
    white-space: pre-wrap;
    background-color: #f5f5f5;
    border: 1px solid #ccc;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
}
pre code {
    padding: 0;
    color: inherit;
    background-color: transparent;
    border: 0;
}
form {
    margin: 0;
}
fieldset {
    padding: 0;
    margin: 0;
    border: 0;
}

legend {
    display: block;
    width: 100%;
    padding: 0;
    margin-bottom: 20px;
    font-size: 21px;
    line-height: 40px;
    color: #333333;
    border: 0;
    border-bottom: 1px solid #e5e5e5;
}
legend small {
    font-size: 15px;
    color: #999999;
}

label,
input,
button,
select,
textarea {
    font-size: 12px;
    font-weight: normal;
    line-height: 20px;
}
input,
button,
select,
textarea {
    font-family: "Microsoft YaHei",Helvetica,Verdana,Arial,Tahoma;
}
button,
input[type="button"],
input[type="reset"],
input[type="submit"] {
    cursor: pointer;
    -webkit-appearance: button;
}

button:active {    
    outline: 0;    
}



table{
    background-color: transparent;
    border-collapse: collapse;
    border-spacing: 0;
}
td{
    vertical-align: middle;
    text-align: left;
}

.clearfix:after {
    visibility: hidden;
    display: block;
    font-size: 0px;
    content: " ";
    clear: both;
    height: 0px;
}
.clearfix{
	*zoom:1;
}
.clear-2 {
	clear: both;
	height: 0px;
	font-size: 0px;
	line-height: 0;
	overflow:hidden;
}
