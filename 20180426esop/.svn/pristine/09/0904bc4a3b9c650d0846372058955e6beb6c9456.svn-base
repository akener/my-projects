                                   /* grpacctTr.find("td:eq(9)").html(result[i].INVOICEFEE / 100);*/

//                                    var invSumVal = sum($("#invoiceSumValue").val(), result[i].INVOICEFEE / 100);
//                                    $("#invoiceSumValue").val(formatMoney("" + invSumVal));
                                    var invList = result[i].INVLIST;

