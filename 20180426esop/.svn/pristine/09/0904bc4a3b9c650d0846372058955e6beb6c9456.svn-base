    /**
     * 查询发票号码
     */
    function invAppr(){
        var originalApplicationNo=$("#originalApplicationNo").val();
        var groupId=$("#groupId").val();
        var codeAndNumber=$("#codeAndNumber").val();
        <!--ESOP清单编号-->
        if (originalApplicationNo == "") {
            return false;
        }
        <!--集团编码-->
        if (groupId == "") {
            return false;
        }
        <!--集团账号-->
        var url = 'UserOrderService';
        var method = "arQryPrePrintInv";
        <!--集团编码-->
        var param={};
        param['GROUPID'] = groupId;
        param['ESOPORDERID'] = originalApplicationNo;

        //moni();

          $.ajax({
              type: 'POST',
              url: '<%=ctxPath %>/boss/postRest',
              data: {
                 url: url,
                 method: method,
                 param: JSON.stringify(param),
                 connectTimeout: 10000,
                 readTimeOut: 30000
             },
            dataType: 'json',
            success: function (data) {
                 var res_code = data.res_code;
                 if (res_code == "0") {
