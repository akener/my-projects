<%@ page contentType="text/html;charset=UTF-8"%>
<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=8">
    <%@ include file="../../pub/base.jsp"%>
    <%@ include file="../../pub/dataTables.jsp"%>
    <%@ include file="../../pub/tableList.jsp"%>
    <title>审批修改集团缴费联系人</title>
</head>
<body class="bg-ff">


<div class="work-order-box overh">
    <div class="row col-14 bg-white">
        <div class="market-name-box pdlr-10 clearfix">
            <span class="fl font-cols"><b style="border: 4px solid #eca100;"></b>集团缴费联系人信息</span>
        </div>
    </div>
    <div class="row col-14">
        <div class="pd-10 pos-rel pos-bg1" style="background-color:#fff;">
            <div class="row">
                <form id="groupDetailInfo">
                    <table class="table-infor-7 width-th" width="100%">
                        <tbody>
                        <tr>
                            <th>集团缴费联系人姓名</th>
                            <td>
                                <input id="link_man4" type="text" class="input-sty04 form-ipt" readonly="true">
                            </td>
                            <th>集团缴费联系人电话</th>
                            <td>
                                <input id="mobile_no4" type="text" class="input-sty04 form-ipt" readonly="true">
                            </td>
                        </tr>

                        <tr>
                            <th>集团缴费联系人地址</th>
                            <td>
                                <input id="address4" type="text" class="input-sty04 form-ipt" readonly="true">

                            </td>
                            <th>集团缴费联系人身份证号</th>
                            <td>
                                <input id="id_number4" type="text" class="input-sty04 form-ipt" readonly="true">

                            </td>
                        </tr>
                        <tr>
                            <th>邮件</th>
                            <td>
                                <input id="email4" type="text" class="input-sty04 form-ipt" readonly="true">

                            </td>
                        </tr>
                        <tr>
                            <th>集团单位名称</th>
                            <td>
                                <input id="union_name4" type="text" class="input-sty04 form-ipt" readonly="true">
                            </td>
                            <th>集团单位编码</th>
                            <td>
                                <input id="union_code4" type="text" class="input-sty04 form-ipt" readonly="true">
                            </td>
                        </tr>
                        <tr>
                            <th>集团账户名称</th>
                            <td>
                                <input id="account_name4" type="text" class="input-sty04 form-ipt" readonly="true">
                            </td>

                            <th>集团账户编码</th>
                            <td>
                                <input id="account_code4" type="text" class="input-sty04 form-ipt" readonly="true">
                            </td>
                        </tr>

                        <tr>
                            <th>审核意见</th>
                            <td>
                                <select id="idea4" class="selector_region input-sty04 form-ipt" onchange="getRemark4()">
                                    <option value="">
                                        -请选择-
                                    </option>
                                    <option value="1">
                                        同意
                                    </option>
                                    <option value="0">
                                        退回
                                    </option>
                                </select>
                            </td>
                        </tr>
                              <tr>
                              <th>备注</th>
                            <td>
                                <textarea id="remark4" name="remark4"></textarea>
                             </td>
                                 </tr>
                              <tr>
                            <th>下一步环节</th>
                            <td>
                                <input id="next_deal4" type="text" class="input-sty04 form-ipt" readonly="true">
                            </td>

                            <th>下一步环节处理人</th>
                            <td>
                                <input id="next_dealName4" type="text" class="input-sty04 form-ipt" readonly="true">
                            </td>
                        </tr>

                  
                        </tbody>
                    </table>
                </form>
            </div>
        </div>
    </div>
</div>
<div class="row mgt-40">
    <div class="btn-box">
        <button class="ui-btn mgr-10" onclick="tuihuicheck()">退回</button>
        <button class="ui-btn mgr-10" onclick="savecheck()">通过</button>
    </div>
</div>
<script type="text/javascript" language="javascript" src="/esop/view/cqDevelopment/elecPack/groupPayContacter.js"></script>
</body>
</html>