}
.window {
	width: 400px;
	border: 1px #94b2bc solid;
	position: absolute;
	z-index: 10001;
	background-color: #FFF;
 filter: progid:DXImageTransform.Microsoft.Shadow(Color=#c5d0d4, Direction=120, strength=3);
}
.window .caption {
	background: #fff url(../../images/5x/block.gif) repeat-x 0px 1px;
	border: 1px solid #fff;
	border-bottom: 0;
	height: 24px;
	text-align: left;
	text-decoration: none;
	position: relative;
}
.window .caption span {
	display: block;
	padding-left: 10px;
	color: #E0F1F9;
	float: left;
}
.window .caption .text {
	background: transparent url(../../images/5x/block.gif) no-repeat scroll 3px -40px;
	color: #2e5b88;
	height: 20px;
	line-height: 22px;
