/*===============common start=============*/
body {
    font:12px SimSun,Arial,Tahoma,Verdana;
	color:#000000;
}
a:link, a:active{
    color:blue;
    text-decoration: none;
}
a:visited{
    color:grey;
    text-decoration: none;
}
a:hover{
    color:red;
    text-decoration: underline;
}

a.a-link:link{
    color:blue;
    text-decoration: none;
}
a.a-link:visited{
    color:grey;
    text-decoration: none;
}
a.a-link:hover{
    color:red;
    text-decoration: underline;
}

a.a-visited:link{
    color:grey;
    text-decoration: none;
}
a.a-visited:visited{
    color:grey;
    text-decoration: none;
}
a.a-visited:hover{
    color:red;
    text-decoration: underline;
}

.red {
    color:#ff0000;
}
.align-right{text-align:right;}
.input td.align-right{text-align:right;}
