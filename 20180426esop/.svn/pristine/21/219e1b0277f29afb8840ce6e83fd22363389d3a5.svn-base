	width: auto;
}
.window .caption a {/*modify*/
	float: right;
	width: 21px;
	margin-right: 5px;
	height: 18px;
	border:none;
	list-style-type: none;
	cursor: pointer;
	z-index: 10;
}
.window .caption a:hover {
	border: 1px #aabbd8 solid;
	background: #dfe9f8;
}
.window .caption .close {
	width: 19px;
	height: 18px;
	position: absolute;
	right: 0px;
	margin:3px 5px 0 0;
	background: url(../../images/5x/block.gif) no-repeat 0px -25px;
	border:none;
}
