package com.esop.workOrderInfo.dto;

import com.esop.common.page.BaseDto;

/**
 * @Description: 客户经理自己任务数据
 * @Date: Created by yuansp on 2017/7/11.
 */
public class CustSerfOrderDto extends BaseDto{
    public String getDT_RowId() {
        return DT_RowId;
    }

    public void setDT_RowId(String DT_RowId) {
        this.DT_RowId = DT_RowId;
    }

    String DT_RowId = "";
    private String  region_name="";//名字
    private String region_no="";//客户经理工号
    private String  pack="";//4g套餐变更任务量
    private String  termi="";//4G终端销售量
    private String  open_amout="";//开户量
    private String  cust="";//4g上网客户数
    private String  broad="";//宽带发展量

    public String getRegion_name() {
        return region_name;
    }

    public void setRegion_name(String region_name) {
        this.region_name = region_name;
    }

    public String getRegion_no() {
        return region_no;
    }

    public void setRegion_no(String region_no) {
        this.region_no = region_no;
    }

    public String getPack() {
        return pack;
    }

    public void setPack(String pack) {
        this.pack = pack;
    }

    public String getTermi() {
        return termi;
    }

    public void setTermi(String termi) {
        this.termi = termi;
    }

    public String getOpen_amout() {
        return open_amout;
    }

    public void setOpen_amout(String open_amout) {
        this.open_amout = open_amout;
    }

    public String getCust() {
        return cust;
    }

    public void setCust(String cust) {
        this.cust = cust;
    }

    public String getBroad() {
        return broad;
    }

    public void setBroad(String broad) {
        this.broad = broad;
    }

    @Override
    public String toString() {
        return "CustSerfOrderDto{" +
                "DT_RowId='" + DT_RowId + '\'' +
                ", region_name='" + region_name + '\'' +
                ", region_no='" + region_no + '\'' +
                ", pack='" + pack + '\'' +
                ", termi='" + termi + '\'' +
                ", open_amout='" + open_amout + '\'' +
                ", cust='" + cust + '\'' +
                ", broad='" + broad + '\'' +
                '}';
    }
}
