}

// 判断纳税人识别号：数字和字母
function judgeTaxpayerCode(code){
    if(!(/^[0-9a-zA-Z]{0,}$/.test(code))){
        alert("纳税人识别号为数字和字母组合！");
        return false;
    }
    return true;
}


function isAllChinese(ob){
    var patrn=/[\u4E00-\u9FA5]|[\uFE30-\uFFA0]/gi;
    if(!patrn.exec(ob)){
        alert("甲方名称至少有文字!");
        return false;
    }
    return true;
}

