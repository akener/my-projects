    if(opporNo==null||$.trim(opporNo)=='null'||$.trim(opporNo)==''){
        alert("商机号为空，无法选择！");
        return;
    }
    var index = layer.load(2, {
        shade: [0.1,'#fff'] //0.1透明度的白色背景
    });
    /*
     var temp = $("#opportunity_no").val();
    var tempList = temp.split(",");
