   //小数转百分数并保留两位小数
    function toPercent(point){
        var str=Number(point*100).toFixed(1);
        str+="%";
        return str;
    }
    function getBusiDepList(){
        $("#epBusiDep tr").each(function(i){
            if(i>=3){
                $(this).empty();
            }
        });


        $.ajax({
            type: 'POST',
            url: '/getEpBusiDepInfo',
            cache : false,
            data:{
                eff_date:$("#eff_date").val()
            },
            success : function(data) {
                if(data.length > 0) {
                    for(var i=0; i<data.length; i++){
                        var trStr = "";
                        trStr+="<tr>";
                        trStr +=
                            "<td>" + data[i].region_name + "</td>" +
                            "<td>" + data[i].day_inc + "</td>" +
                            "<td>" + data[i].year_inc + "</td>" +
                            "<td>" + data[i].goal + "</td>" +
                            "<td>" + toPercent(data[i].comp_rate) + "</td>" +
                            "<td>" + data[i].wid_day_inc + "</td>" +
                            "<td>" + data[i].wid_month_inc + "</td>" +
                            "<td>" + data[i].wid_year_inc + "</td>" +
                            "<td>" + data[i].wid_goal + "</td>" +
                            "<td>" + toPercent(data[i].wid_comp_rate) + "</td>" +
                            "<td>" + data[i].unit_day_inc + "</td>" +
                            "<td>" + data[i].unit_mon_inc + "</td>" +
                            "<td>" + data[i].unit_year_inc + "</td>" +
                            "<td>" + data[i].unit_goal + "</td>" +
                            "<td>" + toPercent(data[i].uint_comp_rate) + "</td>" +
                            "<td>" + data[i].shool_day_inc + "</td>" +
                            "<td>" + data[i].school_month_inc + "</td>" +
                            "<td>" + data[i].school_year_inc + "</td>" +
                            "<td>" + data[i].shool_goal + "</td>" +
                            "<td>" + toPercent(data[i].school_comp_rate) + "</td>" +
                            "<td>" + data[i].net_day_inc + "</td>" +
                            "<td>" + data[i].net_mon_inc + "</td>" +
                            "<td>" + data[i].net_total_inc + "</td>" +
                            "<td>" + data[i].net_goal + "</td>" +
                            "<td>" + toPercent(data[i].net_comp_rate)+ "</td>" +
                            "</tr>";
                        $("#epBusiDep").append(trStr);
                    }
                }else{
                    $("#modal_text").text("没有数据");
                    $("#modalInfo").modal('show');
                    return false;

                }
                // $("#initElement").val(resp);
            },
            error:function(resp){
               // alert("查询数据失败");
                $("#modal_text").text("查询数据失败");
                $("#modalInfo").modal('show');
                return false;
            }
        });
    }
    function qryBusiDepList(){
        table = $('#epBusiDep').DataTable( {
            dom: "Bfrtip",
            destroy:true,
            bLengthChange:true,
            "bFilter": false,
            //"iDisplayLength": 10,
            //"aLengthMenu": [10, 50, 100 ],
            "pageLength": 14,//设置每页显示多少条记录
            "language": {
                "url": "/esop/static/data/Chinese.json"
            }, //多语言配置
            processing: true,
            serverSide: true,
            ajax: {
                url:"/EpBusiDep",
                type:"POST",
                data: {
                    eff_date: $("#eff_date").val()
                }
            },
            columns: [
                { data: "region_name" },
                { data: "day_inc"},
                { data: "year_inc"},
                { data: "goal"},
                { data: "comp_rate"},
                { data: "wid_day_inc"},
                { data: "wid_month_inc"},
                { data: "wid_year_inc"},
                { data: "wid_goal"},
                { data: "wid_comp_rate"},
                { data: "unit_day_inc"},
                { data: "unit_mon_inc"},
                { data: "unit_year_inc"},
                { data: "unit_goal"},
                { data: "uint_comp_rate"},
                { data: "shool_day_inc"},
                { data: "school_month_inc"},
                { data: "school_year_inc"},
                { data: "shool_goal"},
                { data: "school_comp_rate"},
                { data: "net_day_inc"},
                { data: "net_mon_inc"},
                { data: "net_total_inc"},
                { data: "net_goal"},
                { data: "net_comp_rate"}
            ],
            select: true,
            buttons: [
            ]
        } );
    }
</script>
