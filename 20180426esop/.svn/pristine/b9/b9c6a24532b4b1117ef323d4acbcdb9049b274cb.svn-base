    //合并表格上下单元格
    function mergeCell($table, colIndex) {

        $table.data('col-content', ''); // 存放单元格内容
        $table.data('col-rowspan', 1);  // 存放计算的rowspan值  默认为1
        $table.data('col-td', $());     // 存放发现的第一个与前一行比较结果不同td(jQuery封装过的), 默认一个"空"的jquery对象
        $table.data('trNum', $('tbody tr', $table).length); // 要处理表格的总行数, 用于最后一行做特殊处理时进行判断之用

        // 我们对每一行数据进行"扫面"处理 关键是定位col-td, 和其对应的rowspan
        $('tbody tr', $table).each(function(index) {
            // td:eq中的colIndex即列索引
            var $td = $('td:eq(' + colIndex + ')', this);

            // 取出单元格的当前内容
            var currentContent = $td.html();

            // 第一次时走此分支
            if ($table.data('col-content') == '') {

                $table.data('col-content', currentContent);
                $table.data('col-td', $td);

            } else {
                // 上一行与当前行内容相同
                if ($table.data('col-content') == currentContent) {
                    // 上一行与当前行内容相同则col-rowspan累加, 保存新值
                    var rowspan = $table.data('col-rowspan') + 1;
                    $table.data('col-rowspan', rowspan);
                    // 值得注意的是  如果用了$td.remove()就会对其他列的处理造成影响
                    $td.hide();

                    // 最后一行的情况比较特殊一点
                    // 比如最后2行 td中的内容是一样的, 那么到最后一行就应该把此时的col-td里保存的td设置rowspan
                    if (++index == $table.data('trNum'))
                        $table.data('col-td').attr('rowspan', $table.data('col-rowspan'));

                } else { // 上一行与当前行内容不同
                    // col-rowspan默认为1, 如果统计出的col-rowspan没有变化, 不处理
                    if ($table.data('col-rowspan') != 1) {
                        $table.data('col-td').attr('rowspan', $table.data('col-rowspan'));
                    }
                    // 保存第一次出现不同内容的td, 和其内容, 重置col-rowspan
                    $table.data('col-td', $td);
                    $table.data('col-content', $td.html());
                    $table.data('col-rowspan', 1);
                }
            }
        });
    }



    /**
     * 查询boss预打发票信息
     */
    function loadPrePrintInv() {
        $("#table-account tr:not(:first)").each(function () {
            <!--集团编码-->
            var groupId = $("#groupId").val();
            if (groupId == "") {
                return false;
            }
            <!--集团账号-->
            var grpacctTr = $(this);
            var grpacctid = grpacctTr.find("td:eq(2)").html();
            var url = 'UserOrderService';
            var method = "arQryPrePrintInv";
            var begin = new Date();
            begin.setFullYear(begin.getFullYear() - 1);
            var end = new Date();
            var param = {};
            <!--集团编码-->
            param['GROUPID'] = groupId;
            param['GROUPACCTID'] = grpacctid;
            param['BEGINDATE'] = begin.format("yyyyMMdd");
            param['ENDDATE'] = end.format("yyyyMMdd");

            // console.log(JSON.stringify(param));

            $.ajax({
                type: 'POST',
                url: '<%=ctxPath %>/boss/postRest',
                data: {
                    url: url,
                    method: method,
                    param: JSON.stringify(param),
                    connectTimeout: 10000,
                    readTimeOut: 30000
                },
                dataType: 'json',
                success: function (data) {
                    var res_code = data.res_code;
                    if (res_code == "0") {
                        var result = data.result;
                        /*result = [{
                            "ORDERID": "220180116042714100",
                            "INVOICEFEE":"108300"
                        }
                        ];*/
                        if (isNotNull(result)) {
                            for (var i = 0; i < result.length; i++) {
                                var originalApplicationNo = $("#originalApplicationNo").val();
                                if (result[i].ORDERID == originalApplicationNo) {
                                   /* grpacctTr.find("td:eq(9)").html(result[i].INVOICEFEE / 100);*/

//                                    var invSumVal = sum($("#invoiceSumValue").val(), result[i].INVOICEFEE / 100);
//                                    $("#invoiceSumValue").val(formatMoney("" + invSumVal));
                                    var invList = result[i].INVLIST;

                                    /*invList = [{
                                     "INVOICECODE": "123456",
                                     "INVOICENUM": "12345678"
                                     }, {"INVOICECODE": "123456", "INVOICENUM": "12345"}];*/

                                    if (isNotNull(invList)) {
                                        var invListLen = invList.length;
                                        var rpLen = invListLen > 1 ? invListLen : 1;
                                         grpacctTr.find("td:lt(10)").attr("rowspan", rpLen);
                                         grpacctTr.find("td:gt(11)").attr("rowspan", rpLen);

                                        if (invListLen > 0) {
                                            grpacctTr.find("td:eq(10)").html(invList[0].INVOICENUM);
                                            grpacctTr.find("td:eq(11)").html(invList[0].INVOICECODE);
                                            checkFundOccupy(invList[0].INVOICENUM);
                                        }
                                        if (invListLen > 1) {
                                            for (var j = 1; j < invList.length; j++) {
                                                var inv = invList[j];
                                                var invHtml = "<tr class='liweijun'>" +
                                                    "    <td>" + inv.INVOICENUM + "</td>\n" +
                                                    "    <td>" + inv.INVOICECODE + "</td>\n" +
                                                    "</tr>";
                                                grpacctTr.after(invHtml);
                                                checkFundOccupy(inv.INVOICENUM);
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    } else {
                        var errorMessage = data.res_desc;
                        // console.log(errorMessage);
                    }
                }
            });
        });

//        $('input:checkbox').each(function (index, domEle) {
//            $(this).attr('checked', true);
//        });
    }

